<%= form_with model: @login_form, url: sessions_path, method: :post do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email, required: true, min: 3, max: 64 %>

  <%= form.label :password %>
  <%= form.password_field :password, required: true, min: 3, max:64 %>

  <%= form.submit 'Login' %>
<% end %>
